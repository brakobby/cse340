<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">Kobby Motors</a>
  </span>
  <div id="tools">
    <% if (!loggedin) { %>
      <a title="Click to log in" href="/account/login">My Account</a>
    <% } else { %>
      <span>Welcome <%= accountData.account_firstname %></span>
      <a title="Logout" href="/account/logout">Logout</a>
      <a title="Account Management" href="/account/">Manage Account</a>
    <% } %>
  </div>
</header>
